{
  "manifest_version": 2,

  "name": "Hindi word meaning",
  "description": "This extension gives meaning of selected hindi words",
  "version": "1.0",
  "icons": {
          "128": "logo.png" 
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["jquery-2.1.1.min.js","jquery-ui.min.js","testScript.js"],
        "css" : ["jquery-ui.theme.min.css","jquery-ui.structure.min.css"]
    }
	],
"permissions": [
  ]
}
